{
    "$schema": "https://aka.ms/terminal-profiles-schema",
    "actions":
    [
        {
            "command": "find",
            "keys": "ctrl+shift+f"
        },
        {
            "command": "unbound",
            "keys": "shift+space"
        },
        {
            "command": "unbound",
            "keys": "alt+down"
        },
        {
            "command": "unbound",
            "keys": "alt+left"
        },
        {
            "command": "unbound",
            "keys": "alt+right"
        },
        {
            "command": "unbound",
            "keys": "alt+up"
        },
        {
            "command": "unbound",
            "keys": "alt+j"
        }
    ],
    "alwaysShowTabs": false,
    "copyOnSelect": true,
    "defaultProfile": "{00000000-0000-0000-0000-000000000001}",
    "disabledProfileSources":
    [
        "Windows.Terminal.Azure",
        "Windows.Terminal.Wsl"
    ],
    "profiles":
    {
        "defaults":
        {
            "backgroundImage": "c:/users/andrey/pictures/backgrounds/spongebob.gif",
            "backgroundImageAlignment": "center",
            "backgroundImageOpacity": 0.17499999999999999,
            "backgroundImageStretchMode": "uniformToFill",
            "bellStyle": "none",
            "colorScheme": "Tango Dark",
            "fontFace": "Consolas",
            "fontSize": 14,
            "historySize": 0,
            "padding": "0",
            "scrollbarState": "hidden"
        },
        "list":
        [
            {
                "acrylicOpacity": 0.91000000000000003,
                "backgroundImageStretchMode": "none",
                "commandline": "wsl.exe -d Ubuntu-20.04 -- cd ~; ENV=~/.config/sh/login exec sh",
                "experimental.retroTerminalEffect": false,
                "guid": "{00000000-0000-0000-0000-000000000001}",
                "name": "Ubuntu 20.04",
                "padding": "0",
                "useAcrylic": false
            },
            {
                "commandline": "wsl.exe -d Ubuntu-20.04",
