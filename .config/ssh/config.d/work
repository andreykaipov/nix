# vim: ft=sshconfig

Host smart-toaster work
    User andreyk
    HostName 10.0.0.67

# Interactive variant of above
Host work
    RemoteCommand ENV=~/.config/sh/login exec sh
    RequestTTY yes

Host mia atl tor dlas1 plas1 us-east1 na-northeast1
    User andreyk
    SendEnv VAULT_*
Host mia
    ProxyJump smart-toaster
    HostName jumpbox-dev00.mia.ucloud.int
Host dlas1
    ProxyJump smart-toaster
    HostName jumpbox-dlas100.dlas1.ucloud.int
Host tor
    ProxyJump mia
    HostName jumpbox-tor200.tor.ucloud.int
Host atl
    ProxyJump mia
    HostName jumpbox-atl200.atl.ucloud.int
Host plas1
    ProxyJump mia
    HostName jumpbox-plas100.plas1.ucloud.int
Host us-east1
    ProxyJump atl
    Hostname jumpbox-100.us-east1.prod.gcp.int
Host na-northeast1
    ProxyJump tor
    Hostname jumpbox-100.northamerica-northeast1.prod.gcp.int
